{
  "systemName": "Springboot-Microservice",
  "microservices": [
    {
      "baseDir": "$MS_ROOT/cloud-config-server",
      "basePackage": "cloud",
      "microserviceName": "cloud-config-server"
    },
    {
      "baseDir": "$MS_ROOT/cloud-gateway",
      "basePackage": "gateway",
      "microserviceName": "cloud-gateway"
    },
    {
      "baseDir": "$MS_ROOT/department-service",
      "basePackage": "department",
      "microserviceName": "department-service"
    },
    {
      "baseDir": "$MS_ROOT/hystrix-dashboard",
      "basePackage": "hystrix",
      "microserviceName": "hystrix-dashboard"
    },
    {
      "baseDir": "$MS_ROOT/service-registry",
      "basePackage": "registry",
      "microserviceName": "service-registry"
    },
    {
      "baseDir": "$MS_ROOT/user-service",
      "basePackage": "user",
      "microserviceName": "user-service"
    }
  ]
}
