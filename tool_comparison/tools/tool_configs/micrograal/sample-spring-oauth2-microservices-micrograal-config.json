{
  "systemName": "sample-spring-oauth2-microservices",
  "microservices": [
    {
      "baseDir": "$MS_ROOT/account",
      "basePackage": "account",
      "microserviceName": "account"
    },
    {
      "baseDir": "$MS_ROOT/auth",
      "basePackage": "auth",
      "microserviceName": "auth"
    },
    {
      "baseDir": "$MS_ROOT/discovery",
      "basePackage": "discovery",
      "microserviceName": "discovery"
    },
    {
      "baseDir": "$MS_ROOT/gateway",
      "basePackage": "gateway",
      "microserviceName": "gateway"
    }
  ]
}

